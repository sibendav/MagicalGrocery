<Window x:Class="MagicalGrocery.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MagicalGrocery" xmlns:local1="clr-namespace:MagicalGrocery.controls"
        mc:Ignorable="d"
        Title="MainWindow"
        WindowState="Maximized"
        Name="mainWin">
    <Window.Background>
        <ImageBrush ImageSource="/Images/background1.png" />
    </Window.Background>
    <Grid>
        <local1:mainUC x:Name="topWin" VerticalAlignment="Stretch" ></local1:mainUC>
    </Grid>
</Window>
